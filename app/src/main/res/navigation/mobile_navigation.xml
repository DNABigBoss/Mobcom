<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.e.resepjajanankekinian.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" />

    <fragment
        android:id="@+id/navigation_kulkas"
        android:name="com.e.resepjajanankekinian.ui.kulkas.KulkasFragment"
        android:label="@string/title_dashboard"
        tools:layout="@layout/activity_kulkas" />

    <fragment
        android:id="@+id/navigation_bookmark"
        android:name="com.e.resepjajanankekinian.ui.notifications.NotificationsFragment"
        android:label="Bookmark"
        tools:layout="@layout/activity_bookmark" />
    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.e.resepjajanankekinian.profil"
        android:label="Profile"
        tools:layout="@layout/activity_profil" />
